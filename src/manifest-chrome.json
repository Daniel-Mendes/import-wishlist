{
   "action": {
      "default_popup": "popup/popup.html",
      "default_title": "Import Wishlist"
   },
   "background": {
      "service_worker": "background/background.js"
   },
   "content_scripts": [ {
      "js": [ "content/content.js" ],
      "matches": [ "https://auth.gog.com/token*" ]
   } ],
   "description": "Import your Steam's wishlist to GOG.com",
   "host_permissions": [ "https://store.steampowered.com/wishlist/profiles/", "https://steamcommunity.com/*", "https://*.gog.com/*" ],
   "icons": {
      "16": "assets/icons/icon-16.png",
      "48": "assets/icons/icon-48.png",
      "128": "assets/icons/icon-128.png"
   },
   "manifest_version": 3,
   "name": "Import Wishlist",
   "permissions": [ "tabs", "storage", "identity", "webNavigation" ],
   "version": "0.0.3"
}